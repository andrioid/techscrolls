{
  "name": "jetstream",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "dev": "bun --watch --env-file=../../.env index.ts",
    "debug": "bun --inspect --env-file=../../.env index.ts",
    "buildtest": "bun build index.ts --minify --target node --outdir=dist",
    "prod": "bun index.ts",
    "prod:node": "bun run build && node dist/index.ts",
    "prod:deno": "deno run --allow-env --allow-read --allow-ffi --allow-net --unstable-sloppy-imports --env-file=../../.env index.ts"
  },
  "dependencies": {
    "@andrioid/atproto": "workspace:*"
  },
  "devDependencies": {},
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
